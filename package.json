  "peerDependencies": {
    "express": ">=4"
  },

  "devDependencies": {
    "express": "^4.19.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },

  "keywords": [
    "express",
    "response",
    "api",
